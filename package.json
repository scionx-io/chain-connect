{
  "name": "@scionx/chain-connect",
  "version": "1.0.0",
  "description": "Multi-chain wallet connector supporting Ethereum, Solana, and Tron with minimalist Swiss design",
  "type": "module",
  "main": "./dist/chain-connect.cjs",
  "module": "./dist/chain-connect.js",
  "browser": "./dist/chain-connect.umd.js",
  "exports": {
    ".": {
      "import": "./dist/chain-connect.js",
      "require": "./dist/chain-connect.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "package": "vite build && npm pack",
    "example": "vite build && npm pack && cd example && npm install"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@solana/web3.js": "^1.98.4",
    "@tronweb3/tronwallet-adapters": "^1.2.14",
    "ethers": "^6.15.0",
    "mipd": "^0.0.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ethereum",
    "solana",
    "tron",
    "wallet",
    "web3",
    "blockchain",
    "multi-chain",
    "crypto",
    "dapp",
    "connector"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/scionx/chain-connect.git"
  },
  "bugs": {
    "url": "https://github.com/scionx/chain-connect/issues"
  },
  "homepage": "https://github.com/scionx/chain-connect#readme"
}
